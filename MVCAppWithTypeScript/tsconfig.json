{
    "compileOnSave": true,
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "ES2022",
        "outDir": "wwwroot/ts",
        "module": "ES2022"
    },
    "include": [
        "scripts/**/*"
    ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}